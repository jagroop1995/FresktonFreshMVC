@model IEnumerable<FrenktonFresh.Models.ContactTable>

@{
    ViewBag.Title = "QueryList";
}

<h2>View Query Data </h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Message)
        </th>
        <th>Delete</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Message)
        </td>
        <td>
            <h3 class="btn btn-danger">    @Html.ActionLink("Delete", "Delete", new { id = item.id })</h3>
        </td>
    </tr>
}

</table>

<a href="@Url.Action("AdminZone","Admin")"> Back to Admin Zone </a>